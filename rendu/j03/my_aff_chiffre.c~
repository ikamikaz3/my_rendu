int	my_putchar(char c);
int	my_aff_chiffre();

int	my_aff_chiffre()
{
  char chiffre;
  
  chiffre = 48;
  while ( chiffre <= 57 )
    {
      my_putchar(chiffre);
      chiffre++;
    }
}

int	my_putchar(char c)
{
  write(1, &c, 1);
}

int	main()
{
  my_aff_chiffre();
}
